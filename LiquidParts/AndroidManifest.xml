<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.BBsRs.liquidpartsNew"
    android:versionCode="10"
    android:versionName="1.7" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="16" />
    
    <uses-permission android:name="com.android.vending.BILLING" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_settings"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo" >
        <activity
            android:name="com.BBsRs.liquidpartsNew.MainActivity"
            android:label="@string/app_name" android:screenOrientation="portrait">
        </activity>
        
        <activity
            android:name="com.BBsRs.liquidpartsNew.LiquidPartsActivity"
            android:label="@string/app_name" android:screenOrientation="portrait">
            
            <intent-filter android:icon="@drawable/ic_launcher" >
                <action android:name="com.BBsRs.liquidparts.LiquidPartsActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            
        </activity>
        <activity android:name="com.BBsRs.liquidpartsNew.HelpActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>
